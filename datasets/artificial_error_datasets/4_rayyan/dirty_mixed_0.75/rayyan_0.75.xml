            </vioGenQueries>
        </errorPercentages>
    <dirtyStrategies>
      <defaultStrategy>
        <strategy>TypoRandom</strategy>
      </defaultStrategy>
    </dirtyStrategies>
    <randomErrors>
      <tables>
        <table name="rayyan">
          <percentage>0.75</percentage>
          <attributes>
            <attribute>journal_issn</attribute>
            <attribute>journal_title</attribute>
